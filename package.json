{
  "name": "inventory-management-server",
  "version": "0.1.0",
  "description": "Backend for the Inventory Management project.",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev --files=./src/types -r dotenv/config src/index.ts",
    "build": "tsc",
    "test": "jest --coverage",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.json .",
    "lint:fix": "eslint --ignore-path .gitignore --ext .js,.ts,.json . --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "author": "Benedikt Schniepp",
  "license": "MIT",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.7",
    "openid-client": "^5.1.8"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "jest": "^27.5.1",
    "mongodb-memory-server": "^8.4.1",
    "prettier": "^2.6.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
